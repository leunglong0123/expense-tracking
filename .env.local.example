# Application Configuration
NEXT_PUBLIC_APP_NAME=Expense Tracker
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_APP_ENV=development

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3000/api
API_SECRET_KEY=your_secret_key_here

# Gemini API Configuration
# Get your Gemini API key from: https://makersuite.google.com/app/apikey
# Free tier includes access to Gemini Pro Vision for OCR processing
GEMINI_API_KEY=your_gemini_api_key_here

# NextAuth.js Configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_random_nextauth_secret

# Google OAuth Configuration
# Create credentials at https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Google Sheets Configuration
# The ID of your Google Sheet for storing receipt data
GOOGLE_SHEET_ID=your_google_spreadsheet_id

# External Services (for future implementation)
# NEXT_PUBLIC_UPLOAD_API_KEY=your_upload_api_key_here
# NEXT_PUBLIC_OCR_API_KEY=your_ocr_api_key_here
# NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your_ga_id_here

# Database Configuration (for future implementation)
# DATABASE_URL=your_database_connection_string_here 